{
  "name": <%= JSON.stringify(pkgName) %>,
  "version": "1.0.0",
  "description": <%= JSON.stringify(pkgDescription) %>,
  "main": "entry_points/api.js",
  "keywords": <%=
    // extra array wrapper to reach the desired indentation level, slice its leading "[\n  " (leading whitespace striped for OTBS) and trailing "\n]"
    JSON.stringify([keywords], null, 2).slice(4, -2)
  %>,
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= ghUser %>/<%= ghRepo %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= ghUser %>/<%= ghRepo %>/issues"
  },
  "homepage": <%= JSON.stringify(homepage) %>,
  "engines": {
    "node": <%= JSON.stringify(enginesNode) %>
  },
  "files": [
    "dist",
    "!dist/test",
    "entry_points",
    "LICENSE"
  ],
  "scripts": {
    "dev": "gulp",
    "prepublish": "npm install gulp-babel@4 babel-runtime@4 && npm test",
    "test": "gulp build"
  },
  "author": {
    "name": <%= JSON.stringify(authorName) %>,
    "email": <%= JSON.stringify(authorEmail) %>
  },
  "license": <%= JSON.stringify(license) %>,
  "dependencies": {
    "babel-runtime": "^4.7.16"
  },
  "devDependencies": {
    "babel-eslint": "^2.0.2",
    "chalk": "^1.0.0",
    "glob-manipulate": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-babel": "^4.0.1",
    "gulp-eslint": "^0.8.0",
    "gulp-filter": "^2.0.2",
    "gulp-load-plugins": "^0.9.0",
    "gulp-plumber": "^1.0.0",
    "gulp-rename": "^1.2.0",
    "gulp-shell": "^0.4.0",
    "gulp-src-ordered-globs": "^1.0.3",
    "gulp-util": "^3.0.3",
    "gulp-watch": "^4.2.2",
    "merge-stream": "^0.1.7",
    "mocha": "^2.2.1",
    "rimraf": "^2.3.2",
    "should": "^5.2.0",
    "stream-array": "^1.0.1",
    "through2": "^0.6.3",
    "vinyl-paths": "^1.0.0"
  }
}
